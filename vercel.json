{
  "version": 2,
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/auth/:path*", "destination": "https://presh-ai-training-backend-omfryz6zv-presh-ai.vercel.app/auth/:path*" },
    { "source": "/api/:path*", "destination": "https://presh-ai-training-backend-omfryz6zv-presh-ai.vercel.app/api/:path*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}


